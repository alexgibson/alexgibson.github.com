  "version": "1.0.2",
  "description": "Personal blog",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "gulp": "^4.0.2"
  },
  "homepage": "http://alxgbsn.co.uk",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexgibson/alxgbsn.co.uk.git"
  },
